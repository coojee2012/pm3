		[global::System.Data.Linq.Mapping.AssociationAttribute(Name="TC_Prj_Info_TC_BZGD_Record", Storage="_TC_Prj_Info", ThisKey="FPrjId", OtherKey="FId", IsForeignKey=true)]
		public TC_Prj_Info TC_Prj_Info
		{
			get
			{
				return this._TC_Prj_Info.Entity;
			}
			set
			{
				TC_Prj_Info previousValue = this._TC_Prj_Info.Entity;
				if (((previousValue != value) 
							|| (this._TC_Prj_Info.HasLoadedOrAssignedValue == false)))
				{
					this.SendPropertyChanging();
					if ((previousValue != null))
					{
						this._TC_Prj_Info.Entity = null;
						previousValue.TC_BZGD_Record.Remove(this);
					}
					this._TC_Prj_Info.Entity = value;
					if ((value != null))
					{
						value.TC_BZGD_Record.Add(this);
						this._FPrjId = value.FId;
					}
					else
					{
						this._FPrjId = default(string);
					}
					this.SendPropertyChanged("TC_Prj_Info");
				}
			}
		}
		
		public event PropertyChangingEventHandler PropertyChanging;
		
		public event PropertyChangedEventHandler PropertyChanged;
		
		protected virtual void SendPropertyChanging()
		{
			if ((this.PropertyChanging != null))
			{
				this.PropertyChanging(this, emptyChangingEventArgs);
			}
		}
		
		protected virtual void SendPropertyChanged(String propertyName)
		{
			if ((this.PropertyChanged != null))
			{
				this.PropertyChanged(this, new PropertyChangedEventArgs(propertyName));
			}
		}
	}
	
